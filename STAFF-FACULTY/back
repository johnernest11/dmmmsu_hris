                <?php
                    ob_start();
                    include_once "assets/connection.php";
                    $conn = dbConnect();
                    $account = $conn->prepare("SELECT * FROM emp_child INNER JOIN emp_profile WHERE emp_child.employee_id = emp_profile.employee_id AND emp_profile.Id=$_SESSION[Id]");
                    $account->execute();
                    while($result = $account->fetch(PDO::FETCH_ASSOC)){
                                     $childd= $result["child_id"];
                      echo '<tr>
                                    <td>'.$result["child_name"].'</td>
                                    <td>'.$result["child_birthdate"].'</td>
                                    <td> 
                                    <button type="button" onclick="editChild('.$childd.');" class="btn btn-warning"><span class ="fa fa-edit"></span></button>
                                    </td>
                            </tr>';
                    }
                    
